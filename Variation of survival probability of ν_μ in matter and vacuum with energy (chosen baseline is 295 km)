    {
      "cell_type": "code",
      "execution_count": 6,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 487
        },
        "id": "6B7-_N-ZHdMK",
        "outputId": "220f9c78-beb4-4bfc-8d69-3d333c04e8ea"
      },
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 640x480 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "E = np.linspace(0.1, 5, 1000)\n",
        "L = 295\n",
        "del_m_sqrd_31 = 2.507e-3  # in eV^2\n",
        "del_m_sqrd_21 = 7.41e-5  # in eV^2\n",
        "theta12 = np.deg2rad(33.425)\n",
        "theta13 = np.deg2rad(8.58)\n",
        "theta23 = np.deg2rad(45)\n",
        "delta_cp = np.deg2rad(0)\n",
        "rho = 2.3  # density in g/cm^3\n",
        "\n",
        "sin2_2theta_13 = np.sin(2 * theta13) ** 2\n",
        "sin2_2theta_12 = np.sin(2 * theta12) ** 2\n",
        "sin2_2theta_23 = np.sin(2 * theta23)**2\n",
        "cos2_2theta_23 = np.cos(2 * theta23)**2\n",
        "alpha = del_m_sqrd_21 / del_m_sqrd_31\n",
        "c13 = np.cos(theta13)\n",
        "s13 = np.sin(theta13)\n",
        "s2_12 = np.sin(2 * theta12)\n",
        "s4_12 = np.sin(4*theta12)\n",
        "c12 = np.cos(theta12)\n",
        "s12 = np.sin(theta12)\n",
        "c23 = np.cos(theta23)\n",
        "\n",
        "A_values = []\n",
        "A_a_values = []\n",
        "P_mumu_values_m = []\n",
        "P_antimumu_values_m = []\n",
        "P_mumu_values_v = []\n",
        "P_antimumu_values_v = []\n",
        "\n",
        "for E_value in E:\n",
        "    delta = (del_m_sqrd_31 * L * 1.27) / E_value\n",
        "    v = 7.63e-5 * rho * E_value\n",
        "    A = v / del_m_sqrd_31\n",
        "    A_values.append(A)\n",
        "\n",
        "    term1 = sin2_2theta_23* (np.sin(delta)**2)\n",
        "    term2 = alpha*(c12**2)*sin2_2theta_23*delta*(np.sin(2*delta))\n",
        "    term3 = alpha**2*(sin2_2theta_12)*(c23**2)*((np.sin(A*delta)**2)/(A**2))\n",
        "    term4 = (alpha**2)*(c12**4)*(sin2_2theta_23)*(delta**2)*(np.cos(2*delta))\n",
        "    term5 = (1/(2*A))*(A**2)*(sin2_2theta_12)*(sin2_2theta_23)*(((np.sin(delta))*((np.sin(A*delta))/A)*(np.cos(A-1))*delta)-((delta/2)*(np.sin(2*delta))))\n",
        "    term6 = 4*(s13**2)*(s23**2)*((np.sin((A-1)*delta)**2)/((A-1)**2))\n",
        "    term7 = (2/(A-1))*(s13**2)*sin2_2theta_23*(((np.sin(delta))*(np.cos(A*delta))*((np.sin((A-1)*delta))/(A-1)))-((A/2)*delta*(np.sin(2*delta))))\n",
        "    term8 = 2*A*s13*sin2_2theta_12*sin2_2theta_23*np.cos(delta_cp)*np.cos(delta)*((np.sin(A*delta))/A)*((np.sin((A-1)*delta))/(A-1))\n",
        "    term9 = (2/(A-1))*A*s13*sin2_2theta_12*sin2_2theta_23*cos2_2theta_23*(np.cos(delta_cp))*(np.sin(delta))*(A*(np.sin(delta))-(((np.sin(A*delta))/A)*(np.cos(A-1))*delta))\n",
        "    P_mumu_m = 1 - term1+term2-term3-term4+term5-term6-term7-term8+term9\n",
        "    P_mumu_values_m.append(P_mumu_m)\n",
        "\n",
        "    P_mumu_v = 1 - ((((s2_12**2*(c23**4 + (s23**4*s13**2))) + ((1 - (0.5*s2_12**2))*s2_23**2*s13**2) + (s4_12*s2_23*(c23**2 - (s23**2*s13**2))*s13*cdelta_cp)-(s2_12**2*s2_23**2*s13**2*cdelta_cp**2))*(np.sin((del_m_sqrd_21*5.08*L)/(4*E_value)))**2) + (4*(((s23**2*c13**2*((s12**2*c23**2) + (c12**2*s23**2*s13**2) + (0.5*s2_12*s2_23*s13**2*cdelta_cp**2)))*(np.sin((del_m_sqrd_31*5.08*L)/(4*E_value)))**2) + ((s23**2*c13**2*((c12**2*c23**2) + (s12**2*s23**2*s13**2) - (0.5*s2_12*s2_23*s13**2*cdelta_cp**2)))*((np.sin((del_m_sqrd_32*5.08*L)/(4*E_value)))**2)))))\n",
        "    P_mumu_values_v.append(P_mumu_v)\n",
        "\n",
        "    v_a = -(7.63e-5 * rho * E_value)\n",
        "    A_a = v_a / del_m_sqrd_31\n",
        "    A_a_values.append(A_a)\n",
        "\n",
        "    P_antimumu_m = 1 - (alpha ** 2) * sin2_2theta_12 * (np.sin(delta * A_a) ** 2) / (A_a ** 2) - sin2_2theta_13 * (np.sin(delta * (A_a - 1)) ** 2) / ((A_a - 1) ** 2)\n",
        "    P_antimumu_values_m.append(P_antimumu_m)\n",
        "\n",
        "    P_antimumu_v = 1 - ((((s2_12**2*(c23**4 + (s23**4*s13**2))) + ((1 - (0.5*s2_12**2))*s2_23**2*s13**2) + (s4_12*s2_23*(c23**2 - (s23**2*s13**2))*s13*cdelta_cp)-(s2_12**2*s2_23**2*s13**2*cdelta_cp**2))*(np.sin((del_m_sqrd_21*5.08*L)/(4*E_value)))**2) + (4*(((s23**2*c13**2*((s12**2*c23**2) + (c12**2*s23**2*s13**2) + (0.5*s2_12*s2_23*s13**2*cdelta_cp**2)))*(np.sin((del_m_sqrd_31*5.08*L)/(4*E_value)))**2) + ((s23**2*c13**2*((c12**2*c23**2) + (s12**2*s23**2*s13**2) - (0.5*s2_12*s2_23*s13**2*cdelta_cp**2)))*((np.sin((del_m_sqrd_32*5.08*L)/(4*E_value)))**2)))))\n",
        "    P_antimumu_values_v.append(P_antimumu_v)\n",
        "\n",
        "# Plotting\n",
        "plt.plot(E, P_mumu_values_m, label='$P^{matter}_{ν_μ->ν_μ}$')\n",
        "plt.plot(E, P_mumu_values_v, label='$P^{vacuum}_{ν_μ->ν_μ}$')\n",
        "plt.plot(E, P_antimumu_values_v, label='$\\\\overline{P}^{vacuum}_{ν_μ->ν_μ}$')\n",
        "plt.plot(E, P_antimumu_values_m, label='$\\\\overline{P}^{matter}_{ν_μ->ν_μ}$')\n",
        "plt.title(\"Variation of survival probability of $ν_μ$ in matter and vacuum with energy \\n (chosen baseline is 295 km)\\n\")\n",
        "plt.xlabel(\"$E_ν$ in GeV\")\n",
        "plt.ylabel(\"Survival probability\")\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ]
}