          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 640x480 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "#constants\n",
        "E = np.linspace(0.1, 5, 1000)\n",
        "L = 295  # baseline in km\n",
        "del_m_sqrd_31 = 2.507e-3  # in eV^2\n",
        "del_m_sqrd_21 = 7.41e-5  # in eV^2\n",
        "theta_12 = np.deg2rad(33.425)\n",
        "theta_13 = np.deg2rad(8.58)\n",
        "rho = 2.3  # density in g/cm^3\n",
        "sin2_2theta_13 = np.sin(2 * theta_13) ** 2\n",
        "sin2_2theta_12 = np.sin(2 * theta_12) ** 2\n",
        "alpha = del_m_sqrd_21 / del_m_sqrd_31\n",
        "\n",
        "\n",
        "A_values = []\n",
        "P_ee_values = []\n",
        "\n",
        "for E_value in E:\n",
        "    delta = (del_m_sqrd_31 * L * 1.27) / E_value\n",
        "    v = 7.63e-5 * rho * E_value  # matter potential in units of eV^2 / (g/cm^3 * GeV)\n",
        "    A = v / del_m_sqrd_31\n",
        "    A_values.append(A)\n",
        "    P_ee = 1 - (alpha ** 2) * sin2_2theta_12 * (np.sin(delta * A) ** 2) / (A ** 2)- sin2_2theta_13 * (np.sin(delta * (A - 1)) ** 2) / ((A - 1) ** 2)\n",
        "    P_ee_values.append(P_ee)\n",
        "\n",
        "plt.plot(E, P_ee_values)\n",
        "plt.title(\"Variation of survival probability of $ν_e$ in matter with energy \\n (chosen baseline is 295 km)\\n\")\n",
        "plt.xlabel(\"$E_ν$ in GeV\")\n",
        "plt.ylabel(\"$P^{matter}_{ν_e->ν_e}$\")\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ]
}
