      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 640x480 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "E = np.arange(0.1, 5, 0.01)\n",
        "L = 295  # baseline in km\n",
        "del_m_sqrd_31 = 2.507e-3  # in eV^2\n",
        "del_m_sqrd_21 = 7.41e-5  # in eV^2\n",
        "theta_12 = np.deg2rad(33.425)\n",
        "theta_13 = np.deg2rad(8.58)\n",
        "theta_23 = np.deg2rad(42.3)\n",
        "delta_cp = np.deg2rad(45)\n",
        "rho = 2.3  # density in g/cm^3\n",
        "\n",
        "s2_12 = np.sin(2 * theta_12)\n",
        "c_23 = np.cos(theta_23)\n",
        "s_13 = np.sin(theta_13)\n",
        "s_23 = np.sin(theta_23)\n",
        "s2_23 = np.sin(2 * theta_23)\n",
        "alpha = del_m_sqrd_21 / del_m_sqrd_31\n",
        "A_factor = 7.63e-5  # in units of eV^2 / (g/cm^3 * GeV)\n",
        "\n",
        "A_values = []\n",
        "P_emu_values = []\n",
        "\n",
        "for i in range(len(E)):\n",
        "    delta = (del_m_sqrd_31 * L * 1.27) / E[i]\n",
        "    v = A_factor * rho * E[i]\n",
        "    A = v / del_m_sqrd_31\n",
        "    A_values.append(A)\n",
        "    term1 = (alpha ** 2) * (s2_12 ** 2) * (c_23 ** 2) * (np.sin(A * delta) ** 2) / (A ** 2)\n",
        "    term2 = 4 * (s_13 ** 2) * (s_23 ** 2) * (np.sin((A - 1) * delta) ** 2) / ((A - 1) ** 2)\n",
        "    term3 = 2 * alpha * s_13 * s2_12 * s2_23 * np.cos(delta - delta_cp) * (np.sin(A * delta) / A) * (np.sin((A - 1) * delta) / (A - 1))\n",
        "    P_emu = term1 + term2 + term3\n",
        "    P_emu_values.append(P_emu)\n",
        "\n",
        "plt.plot(E, P_emu_values)\n",
        "plt.title(\"Variation of transition probability of $ν_e$ to $ν_μ$ in matter with energy \\n (chosen baseline is 295 km)\\n\")\n",
        "plt.xlabel(\"$E_ν$ in GeV\")\n",
        "plt.ylabel(\"$P^{matter}_{ν_e->ν_μ}$\")\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ]
