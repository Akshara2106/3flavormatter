    {
      "cell_type": "code",
      "execution_count": 5,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 487
        },
        "id": "6B7-_N-ZHdMK",
        "outputId": "37016b2d-2acf-4320-cb2c-160dba446253"
      },
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 640x480 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "E = np.arange(0.1, 4.5, 0.01)\n",
        "L = 810  # baseline in km\n",
        "del_m_sqrd_31 = 2.507e-3  # in eV^2\n",
        "del_m_sqrd_32 = 2.507e-3  # in eV^2\n",
        "del_m_sqrd_21 = 7.41e-5  # in eV^2\n",
        "theta12 = 33.425\n",
        "theta13 = 8.58\n",
        "theta23 = 42.3\n",
        "c13 = np.cos(np.deg2rad(theta13))\n",
        "s13 = np.sin(np.deg2rad(theta13))\n",
        "c12 = np.cos(np.deg2rad(theta12))\n",
        "s12 = np.sin(np.deg2rad(theta12))\n",
        "c2_13 = np.cos(np.deg2rad(2*theta13))\n",
        "c4_13 = np.cos(np.deg2rad(4*theta13))\n",
        "s2_13 = np.sin(np.deg2rad(2*theta13))\n",
        "s4_13 = np.sin(np.deg2rad(4*theta13))\n",
        "c23 = np.cos(np.deg2rad(theta23))\n",
        "s23 = np.sin(np.deg2rad(theta23))\n",
        "c2_23 = np.cos(np.deg2rad(2*theta23))\n",
        "c4_23 = np.cos(np.deg2rad(4*theta23))\n",
        "s2_23 = np.sin(np.deg2rad(2*theta23))\n",
        "s4_23 = np.sin(np.deg2rad(4*theta23))\n",
        "s2_12 = np.sin(np.deg2rad(2 * theta12))\n",
        "c2_12 = np.cos(np.deg2rad(2 * theta12))\n",
        "delta_cp = np.deg2rad(45)\n",
        "cdelta_cp = np.cos(np.deg2rad( delta_cp))\n",
        "sdelta_cp = np.sin(np.deg2rad( delta_cp))\n",
        "\n",
        "rho = 2.84  # density in g/cm^3\n",
        "alpha = del_m_sqrd_21 / del_m_sqrd_31\n",
        "A_factor = 7.63e-5  # in units of eV^2 / (g/cm^3 * GeV)\n",
        "\n",
        "A_values = []\n",
        "A_values_a = []\n",
        "P_emu_values_m = []\n",
        "P_antiemu_values_v = []\n",
        "P_antiemu_values_m = []\n",
        "P_emu_values_v = []\n",
        "\n",
        "for i in range(len(E)):\n",
        "    delta = (del_m_sqrd_31 * L * 1.27) / E[i]\n",
        "    v = A_factor * rho * E[i]\n",
        "    A = v / del_m_sqrd_31\n",
        "    A_values.append(A)\n",
        "    term1 = (alpha ** 2) * (s2_12 ** 2) * (c23 ** 2) * (np.sin(A * delta) ** 2) / (A ** 2)\n",
        "    term2 = 4 * (s13 ** 2) * (s23 ** 2) * (np.sin((A - 1) * delta) ** 2) / ((A - 1) ** 2)\n",
        "    term3 = 2 * alpha * s13 * s2_12 * s2_23 * np.cos(delta - delta_cp) * (np.sin(A * delta) / A) * (np.sin((A - 1) * delta) / (A - 1))\n",
        "    P_emu_m = term1 + term2 + term3\n",
        "    P_emu_values_m.append(P_emu_m)\n",
        "    P_antiemu_v = -4*(-0.25*s2_12*c13**2*(s2_12*(c23**2-s23**2*s13**2)+c2_12*s2_23*s13*cdelta_cp)*(np.sin((del_m_sqrd_21*5.064*L)/(4*E[i])))**2-(c12*s23*c13**2*s13*(c12*s23*s13+s12*c23*cdelta_cp))*(np.sin((del_m_sqrd_31*5.064*L)/(4*E[i])))**2-(s12*s23*c13**2*s13*(s12*s13*s23-c12*c23*cdelta_cp))*(np.sin((del_m_sqrd_32*5.064*L)/(4*E[i])))**2)-2*(c12*s23*s13*sdelta_cp*s12*c23*c2_13)*(-np.sin((del_m_sqrd_31*5.064*L)/(2*E[i]))+np.sin((del_m_sqrd_32*5.064*L)/(2*E[i]))+np.sin((del_m_sqrd_21*5.064*L)/(2*E[i])))\n",
        "    P_antiemu_values_v.append(P_antiemu_v)\n",
        "    v_a = -(A_factor * rho * E[i])\n",
        "    A_a = v_a / del_m_sqrd_31\n",
        "    A_values_a.append(A_a)\n",
        "    term1a = (alpha ** 2) * (s2_12 ** 2) * (c23 ** 2) * (np.sin(A_a * delta) ** 2) / (A_a ** 2)\n",
        "    term2a = 4 * (s13 ** 2) * (s23 ** 2) * (np.sin((A_a - 1) * delta) ** 2) / ((A_a - 1) ** 2)\n",
        "    term3a = 2 * alpha * s13 * s2_12 * s2_23 * np.cos(delta - delta_cp) * (np.sin(A_a * delta) / A_a) * (np.sin((A_a - 1) * delta) / (A_a - 1))\n",
        "    P_antiemu_m = term1a + term2a + term3a\n",
        "    P_antiemu_values_m.append(P_antiemu_m)\n",
        "    P_emu_v = -4*((-0.25*s2_12*c13**2*(s2_12*(c23**2-s23**2*s13**2)+c2_12*s2_23*s13*cdelta_cp))*(np.sin((del_m_sqrd_21*5.064*L)/(4*E[i])))**2-c12*s23*c13**2*s13*(c12*s23*s13+s12*c23*cdelta_cp)*(np.sin((del_m_sqrd_31*5.064*L)/(4*E[i])))**2-s12*s23*c13**2*s13*(s12*s23*s13-c12*c23*cdelta_cp)*(np.sin((del_m_sqrd_32*5.064*L)/(4*E[i])))**2)+2*(c12*s23*s13*sdelta_cp*s12*c23*c2_13)*(np.sin((del_m_sqrd_31*5.064*L)/(2*E[i]))-np.sin((del_m_sqrd_32*5.064*L)/(2*E[i]))-np.sin((del_m_sqrd_21*5.064*L)/(2*E[i])))\n",
        "    P_emu_values_v.append(P_emu_v)\n",
        "\n",
        "plt.plot(E,P_antiemu_values_m,label='$\\overline{P}^{matter}_{ν_e->ν_μ}$')\n",
        "plt.plot(E,P_emu_values_m,label='$P^{matter}_{ν_e->ν_μ}$')\n",
        "plt.plot(E,P_emu_values_v,label='$P^{vacuum}_{ν_e->ν_μ}$')\n",
        "plt.plot(E,P_antiemu_values_v,label='$\\overline{P}^{vacuum}_{ν_e->ν_μ}$')\n",
        "plt.title(\"Variation of transition probability of $ν_e$ to $ν_μ$ in matter and vacuum with energy \\n (chosen baseline is 810 km)\\n\")\n",
        "plt.xlabel(\"$E_ν$ in GeV\")\n",
        "plt.ylabel(\"Transition probability\")\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ]
    }
  ]
}
